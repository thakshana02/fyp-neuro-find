
# Don't ignore gitattributes
!.gitattributes

# Ignore everything
*

# Don't ignore deployment files
!/Aptfile
!runtime.txt
!Procfile

# Allow frontend folder and its contents
!/frontend/
!/frontend/**
/frontend/node_modules/

# Only allow specific files in script folder
!/script/
/script/*
!/script/app.py
!/script/subclass.py
!/script/cv_utils.py
# DO NOT include the .h5 files

# Allow README and other important root files
!README.md
!requirements.txt
!.gitignore

# Ignore common unwanted files
.DS_Store
*.log
__pycache__/
